<!-- Tiny1.ui.xml -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:crud='urn:import:com.jgoo.client.crud'>
	<g:VerticalPanel ui:field='crudPanel' styleName='crudPanel'>
		<g:HorizontalPanel styleName='spacedOutId'>
			<g:Label>Id:</g:Label>
			<g:Label ui:field='id'></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName='spacedOut'>
			<g:Label>Name</g:Label>
			<g:TextBox ui:field="name"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName='spacedOut'>
			<g:Label>Food Type</g:Label>
			<g:TextBox ui:field="foodType"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName='spacedOut'>
			<g:Label>Order Online</g:Label>
			<g:TextBox ui:field="orderOnline"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName='spacedOut'>
			<g:Label>Webiste</g:Label>
			<g:TextBox ui:field="website"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName='spacedOut'>
			<g:Label>Phone</g:Label>
			<g:TextBox ui:field="phone"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName='spacedOut'>
			<g:Label>Address</g:Label>
			<g:TextBox ui:field="address"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName='spacedOut'>
			<g:Label>City</g:Label>
			<g:TextBox ui:field="city"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName='spacedOut'>
			<g:Label>State</g:Label>
			<g:TextBox ui:field="state"></g:TextBox>
		</g:HorizontalPanel>
		<g:HorizontalPanel styleName='spacedOut'>
			<g:Label>Zip</g:Label>
			<g:TextBox ui:field="zip"></g:TextBox>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>